{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "restart": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "node",
            "request": "attach",
            "restart": true,
            "name": "Attach Program Debug",
            "protocol": "inspector"
        }
    ]
}